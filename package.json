  "scripts": {
    "test": "jest",
    "build": "rimraf dist && rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "rimraf storybook-static && storybook build",
    "eslint": "eslint -c .eslintrc.json --max-warnings 0 .",
    "prettier-check": "prettier . --check",
    "prettier-fix": "prettier . --write",
    "test-storyshots": "jest storybook.test.ts",
    "test-storyshots-ci": "npm run test-storyshots -- --watchAll false",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006 --silent\" \"wait-on http://localhost:6006 && npm run test-storybook\"",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "local-package": "rimraf *.tgz && npm run build && npm pack"
  },
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
